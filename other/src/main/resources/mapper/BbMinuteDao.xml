<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whp.usdtbb.other.Dao.BbMinuteDao">

    <!--根据用户名查询用户信息 仅程序内部使用-->
    <select id="getMinuteSelect" resultType="map">
        select * from bb_minute where state=0 and isdel=0 ORDER BY `minute` asc
    </select>

</mapper>